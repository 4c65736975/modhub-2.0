{
  "manifest_version": 3,
  "name": "ModHub 2.0",
  "version": "1.0.1",
  "description": "New modhub look and functionality",
  "author": "4c65736975",
  "icons": {
    "16": "/icons/modHub_2_0_icon_16.png",
    "32": "/icons/modHub_2_0_icon_32.png",
    "48": "/icons/modHub_2_0_icon_48.png",
    "128": "/icons/modHub_2_0_icon_128.png"
  },
  "permissions": ["scripting", "activeTab"],
  "content_scripts": [
    {
      "js" : ["index.js"],
      "matches": ["https://*.farming-simulator.com/*"]
    }
  ],
  "action": {

  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [{
    "matches": ["<all_urls>"]
  }],
  "homepage_url": "https://github.com/4c65736975"
}